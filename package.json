{
  "name": "@thomann/sourcemap-service",
  "version": "2.0.0",
  "description": "service for up-/downloading artifacts\n",
  "main": "index.js",
  "scripts": {
    "start": "set \"NODE_ENV=production\" & node index.js",
    "build": "tsc --project tsconfig.prod.json",
    "inspect": "node --inspect node_modules/.bin/micro-dev",
    "watch": "tsc --watch",
    "coverage": "nyc npm test",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register/transpile-only --full-trace 'test/**/*.ts'",
    "test:watch": "mocha -w"
  },
  "author": {
    "name": "Oleg Kamlowski",
    "email": "oleg.kamlowski@thomann.de",
    "url": "https://thomann.de"
  },
  "license": "ISC",
  "dependencies": {
    "fsbr": "^1.0.10",
    "sevl": "^0.1.3",
    "teda": "^0.1.4",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4",
    "vano": "^0.1.3"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "supertest": "^6.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thomn/sourcemap-service.git"
  },
  "keywords": [
    "sourcemaps",
    "micro",
    "service"
  ]
}
